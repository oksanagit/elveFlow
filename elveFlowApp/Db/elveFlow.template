record(ao,"$(P)$(R):setP1") {
    field(PINI, "YES")
    field(DTYP, "asynFloat64")
    field(OUT,  "@asyn($(PORT),$(ADDR))EF_SET_PRESSURE")
    field(DRVL, "$(DRVL)")
    field(DRVH, "$(DRVH)")
    field(PREC, "$(PREC)")
    field(VAL,  "0")
}

record(ai,"$(P)$(R):getP1")
{
    field(SCAN, "1 second")
    field(DTYP, "asynFloat64")
    field(INP,  "@asyn($(PORT),$(ADDR))EF_GET_PRESSURE")
    field(PREC, "$(PREC)")
}

record(ai,"$(P)$(R):getSensor1")
{
    field(SCAN, "1 second")
    field(DTYP, "asynFloat64")
    field(INP,  "@asyn($(PORT),$(ADDR))EF_GET_FLOW")
    field(PREC, "$(PREC)")
}